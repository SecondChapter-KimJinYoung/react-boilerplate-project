{
  "name": "react-boilerplate-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "vite --mode development",
    "start:prod": "vite --mode production",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "quality": "yarn type-check && yarn lint && yarn format:check",
    "quality:fix": "yarn lint:fix && yarn format",
    "predeploy": "yarn quality && yarn build",
    "deploy:check": "yarn type-check && yarn lint && yarn format:check && yarn build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.8",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "pretendard": "^1.3.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-checker": "^0.12.0"
  },
  "resolutions": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
